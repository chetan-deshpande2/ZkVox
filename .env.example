# ZKVox (ZK DAO) Environment Variables
# Copy this to .env and fill in your details

# Private key of the deployer/relayer wallet
PRIVATE_KEY=0xabc...

# RPC URL for Sepolia Testnet
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your-api-key

# RPC URL for Polygon Amoy
AMOY_RPC_URL=https://polygon-amoy.g.alchemy.com/v2/your-api-key

# ETHERSCAN (or Polygonscan) API KEY for contract verification
ETHERSCAN_API_KEY=your-api-key
